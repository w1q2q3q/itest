<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
    <class name="com.itest.entity.Tast" table="Tast">  
  
        <id name="id" column="id" type="int">  
            <generator class="native"></generator>  
        </id>    
        <property name="cassid" column="cassid" type="int"></property>  
        <property name="number" column="number" type="int"></property>  
        <property name="date" column="date" type="string"></property>
        <property name="zhu" column="zhu" type="string"></property>
        <property name="method" column="method" type="int"></property>
    </class>  
</hibernate-mapping>  
