<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
    <class name="com.itest.entity.Testtask" table="testtask">  
  
        <id name="id" column="id" type="int">  
            <generator class="native"></generator>  
        </id>    
        <property name="number" column="number" type="int"></property>
        <property name="name" column="name" type="string"></property>  
        <property name="describev" column="describev" type="string"></property> 
        <property name="date" column="date" type="string"></property> 
        <property name="zhu" column="zhu" type="string"></property>   
    </class>  
</hibernate-mapping>  
