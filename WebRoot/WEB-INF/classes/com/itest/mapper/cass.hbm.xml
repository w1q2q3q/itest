<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
    <class name="com.itest.entity.cass" table="cass">  
  
        <id name="id" column="id" type="int">  
            <generator class="native"></generator>  
        </id>   
        <property name="ylid" column="ylid" type="int"></property> 
        <property name="number" column="number" type="int"></property>  
        <property name="cassid" column="cassid" type="int"></property>
        <property name="zhuid" column="zhuid" type="int"></property>
        <property name="name" column="name" type="string"></property>  
        <property name="describev" column="describev" type="string"></property>
        <property name="zhaungtai" column="zhaungtai" type="int"></property>
        <property name="aeer" column="aeer" type="int"></property>
        <property name="data" column="data" type="string"></property>
        <property name="xiangyintext" column="xiangyintext" type="text"></property>
        <property name="assertc" column="assertc" type="text"></property>
        <property name="beforeheader" column="beforeheader" type="text"></property>
        <property name="afterheader" column="afterheader" type="text"></property>
        <property name="method" column="method" type="int"></property>
    </class>  
</hibernate-mapping>  
